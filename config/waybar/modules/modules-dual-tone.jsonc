        "on-click-right": "kitty -e nmtui"
    },

    // network speed
    "network#speed": {
        // "interface": "wlp2*", // (Optional) To force the use of this interface
        "format": "<span foreground='#1e1e2e'>ÔÄô {bandwidthDownBytes}</span> <span foreground='#11111b'>ÔÇì {bandwidthUpBytes}</span>",
        "format-disconnected": "Û∞ñ™ ",
        "tooltip": false,
        "interval": 1,
        "max-length": 30,
    	"format-icons": [
    		"Û∞§Ø", "Û∞§ü", "Û∞§¢", "Û∞§•", "Û∞§®"
    	],
        "on-click-right": "kitty -e nmtui"
    },

    "idle_inhibitor": {
        "format": "{icon}",
	"start-activated": true,
        "format-icons": {
            "activated": "Ó∞ï",
            "deactivated": "Û∞õä"
        },
        "tooltip-format-activated":"Caffeine Mode Active",
        "tooltip-format-deactivated":"Caffeine Mode Inactive"
    },

    "hyprland/window": {
        "format": "{}",
        "separate-outputs": true,
        "rewrite": {
            "(.*)${USER}@${set_sysname}:(.*)": "$1 Ôíâ :$2",
            "(.*) ‚Äî Mozilla Firefox": "$1 Û∞àπ",
            "(.*)Mozilla Firefox": "$1 Firefox Û∞àπ",
            "(.*) - Visual Studio Code": "$1 Û∞®û",
            "(.*)Visual Studio Code": "$1 Code Û∞®û",
            "(.*) ‚Äî Dolphin": "$1 Û∞âã",
            "(.*)Spotify.*": "$1 Spotify Û∞ìá",
            "(.*)Steam": "$1 Steam Û∞ìì",
            "(.*) - Discord": "$1 Ôáø ",
            "(.*?)-\\s*YouTube(.*)": "$1Û∞óÉ", 
            "(.*?\\s)YouTube(.*)": "$1YouTube Û∞óÉ"
        },
        "max-length": 50
    },


    "hyprland/language": {
        "interval": 1,
        "format": "Ó©• {}",
        "format-uk": "Uk",
        "format-en": "US"
    },

    "hyprland/workspaces": {
        "disable-scroll": true,
        "all-outputs": true,
        "on-click": "activate",
        "on-scroll-up": "hyprctl dispatch workspace e-1",
        "on-scroll-down": "hyprctl dispatch workspace e+1",
        "format-icons": {
            //  "default": "ÔÑë"
          },
        "persistent-workspaces": {
            "1": [],
            "2": [],
            "3": []
        }
    },

    "custom/cpu-icon": {
        "format": "Ó∞ô",
        "tooltip": false
    },

    "cpu": {
        "interval": 10,
        "format": "{usage}%"
    },

    "custom/disk-icon": {
        "format": "ÔÉá ",
        "tooltip": false
    },

    "disk": {
        "interval": 30,
        "format": "{free}",
        "path": "/",
        "tooltip": true,
        "tooltip-format": "Storage - {used} used out of {total} on {path} ({percentage_used}%)"
    },

    "custom/memory-icon": {
        "format": "Û∞æÖ ",
        "tooltip": false
    },

    "memory": {
        "interval": 2,
        "format": "{used}GB",
        "max-length": 10,
        "tooltip": true,
        "tooltip-format": "Óâ¶ {used:0.1f}GB/{total:0.1f}GB"
    },

    "backlight": {
        "interval": 2,
        "align": 0,
        "rotate": 0,
        "format": "{icon} {percent}%",
        "format-icons": [
            "Óèï ",
            "Óèó ",
            "Óèö ",
            "Û∞Éù ",
            "Û∞Éû ",
            "Û∞Éü ",
            "Û∞É† "
        ],
        "tooltip-format": "backlight {percent}%",
        "icon-size": 10,
        "on-click": "",
        "on-click-middle": "",
        "on-click-right": "",
        "on-update": "",
        "on-scroll-up": "~/.config/hypr/scripts/brightness.sh up",
        "on-scroll-down": "~/.config/hypr/scripts/brightness.sh down",
        "smooth-scrolling-threshold": 1
    },

    "tray": {
        "icon-size": 18,
        "spacing": 10
    },

    "clock": {
        "format": "ÔÄó {:%I:%M %p}",
        "format-alt": "Û∞É≠ {:%d¬∑%m¬∑%Y}",
        "on-clock-right": "exec gnome-calendar",
        "tooltip": true,
        "tooltip-format": "<span>{calendar}</span>",
        "calendar": {
            "mode": "month",
            "mode-mon-col": 3,
            "on-scroll": 1,
            "on-click-right": "mode",
            "format": {
                "months": "<span color='#ffead3'><b>{}</b></span>",
                "weekdays": "<span color='#ffcc66'><b>{}</b></span>",
                "today": "<span color='#ff6699'><b>{}</b></span>"
            }
        },
        "actions": {
            "on-click-right": "mode",
            "on-click-forward": "tz_up",
            "on-click-backward": "tz_down",
            "on-scroll-up": "shift_down",
            "on-scroll-down": "shift_up"
        }
    },

    "battery": {
	//"interval": 5,
	"align": 0,
	"rotate": 0,
	"full-at": 100,
	"design-capacity": false,
	"states": {
		"good": 95,
		"warning": 30,
		"critical": 15
	},
	"format": "{icon} {capacity}%",
	"format-charging": "ÔÉß {capacity}%",
	"format-plugged": "Û±òñ {capacity}%",
	"format-alt-click": "click",
	"format-full": "{icon} Full",
	"format-alt": "{icon} {time}",
	"format-icons": [
		"Û∞Çé", "Û∞Å∫", "Û∞Åª", "Û∞Åº", "Û∞ÅΩ", "Û∞Åæ", "Û∞Åø", "Û∞ÇÄ", "Û∞ÇÅ", "Û∞ÇÇ", "Û∞Åπ"
	    ],
	"format-time": "{H}h {M}min",
	"tooltip": true,
	"tooltip-format": "{timeTo} {power}w"
    },

    "custom/pulseaudio_icon": {
        "format": "{icon}",
        "tooltip": "false",
        "on-click": "~/.config/hypr/scripts/volumecontrol.sh --toggle",
        "on-click-right": "pavucontrol",
        "on-scroll-up": "~/.config/hypr/scripts/volumecontrol.sh --inc",
        "on-scroll-down": "~/.config/hypr/scripts/volumecontrol.sh --dec",
        "scroll-step": 5,
        "format-icons": {
            "headphone": "ÔÄ•",
            "hands-free": "ÔÄ•",
            "headset": "ÔÄ•",
            "phone": "ÔÇï",
            "default": ["ÔÄ® "]
        }
    },

    "pulseaudio": {
        "format": "{volume}%",
        "format-muted": "Ôë¶",
        "on-click": "~/.config/hypr/scripts/volumecontrol.sh --toggle",
        "on-click-right": "pavucontrol",
        "on-scroll-up": "~/.config/hypr/scripts/volumecontrol.sh --inc",
        "on-scroll-down": "~/.config/hypr/scripts/volumecontrol.sh --dec",
        "scroll-step": 5,
        "format-icons": {
            "headphone": "ÔÄ•",
            "hands-free": "ÔÄ•",
            "headset": "ÔÄ•",
            "phone": "ÔÇï",
            "default": ["ÔÄ¶", "ÔÄß", "ÔÄ® "]
        }
    },

    "custom/pulse_mic_icon": {
        "format": "ÔÑ∞ "
    },

    "pulseaudio#microphone": {
        "format": "{format_source}",
        "format-source": "{volume}%",
        "format-source-muted": "ÔÑ±",
        "on-click": "pamixer --default-source -t",
        "on-click-right": "pavucontrol",
        "on-scroll-up": "pamixer --default-source -i 5",
        "on-scroll-down": "pamixer --default-source -d 5",
        "scroll-step": 5
    },

    "mpris": {
        "interval": 2,
        "format": "{player_icon}  {status_icon}",
        "format-paused": "{player_icon}  {status_icon}",
        "tooltip": true,
        "tooltip-format": "{dynamic}",
        "on-click": "playerctl play-pause",
        "on-click-middle": "playerctl previous",
        "on-click-right": "playerctl next",
        "scroll-step": 5.0,
        "on-scroll-up": "~/.config/hypr/scripts/volumecontrol.sh --inc",
        "on-scroll-down": "~/.config/hypr/scripts/volumecontrol.sh --dec",
        "smooth-scrolling-threshold": 1,
        "player-icons": {
            "chromium": "Ôâ®",
            "default": "ÔÄù",
            "firefox": "Ôâ©",
            "kdeconnect": "Ôòü",
            "mopidy": "ÔÄÅ",
            "mpv": "Û∞êπ",
            "spotify": "ÔÜº",
            "vlc": "Û∞ïº"
        },
        "status-icons": {
            "paused": "Û∞è§ ",
            "playing": "ÔÅã ",
            "stopped": "ÔÅç"
        },
        "max-length": 30
